#lambda expression (anonymous function)


##########   NORMAL   ########
def add(a,b):
    return a+b
print(add(2,3)) ##o/p:5

print(add)      #o/p:<function add at 0x039D5930>


#########   LAMBDA   ########
add2=lambda a,b: a+b
print(add2(2,3))    ##o/p:5
print(add2)  #o/p:<function <lambda> at 0x010FD978>



multiply=lambda a,b: a*b
print(multiply(2,3))    #o/p:6


